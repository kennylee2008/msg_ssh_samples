<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.com.leadfar.msg.model.SendMessage" table="t_sendmsg">
		<id name="id">
			<generator class="native"/>
		</id>
		<property name="content" type="text"/>
		<property name="receiveNames"/>
		<property name="sendTime"/>
		<property name="title"/>
		<many-to-one name="sender"></many-to-one>
	</class>
</hibernate-mapping>